<%= form_with url: url, method: method, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :breed %>
  <%= f.text_field :breed %>

  <%= f.label :age %>
  <%= f.number_field :age %>

  <%= f.label :adoptable %>
  <%= f.check_box :adoptable, value: value_from_new %>

  <%= f.submit %>
<% end %>
